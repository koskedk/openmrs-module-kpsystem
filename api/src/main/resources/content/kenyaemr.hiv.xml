<!--suppress SpringFacetInspection -->
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<!-- ===================== Program ===================== -->

	<bean id="kpsystem.hiv.program" class="org.openmrs.module.kpcore.program.ProgramDescriptor">
		<property name="targetUuid" value="${metadata.program.HIV}" />
		<property name="eligibilityCalculation" value="org.openmrs.module.kpsystem.calculation.library.hiv.EligibleForHivProgramCalculation" />
		<property name="defaultEnrollmentForm" ref="kpsystem.hiv.form.enrollment" />
		<property name="defaultCompletionForm" ref="kpsystem.hiv.form.completion" />
		<property name="visitForms">
			<set>
				<ref bean="kpsystem.hiv.form.hivGreenCard" />
				<!--<ref bean="kpsystem.hiv.form.clinicalEncounterAddendum" />-->
				<ref bean="kpsystem.common.form.tbScreening" />
				<ref bean="kpsystem.common.form.geneXpert" />
				<ref bean="kpsystem.hiv.form.artFastTrackForm"/>
			</set>
		</property>
		<property name="reports">
			<set>
				<ref bean="kpsystem.hiv.report.qi.adult.c.tx" />
				<ref bean="kpsystem.hiv.report.qi.peds.c.tx" />
				<!--<ref bean="kpsystem.hiv.report.qi.adult.emtct" />-->
				<ref bean="kpsystem.hiv.report.lostToFollowUp" />
				<ref bean="kpsystem.hiv.report.neverScreenedForTb" />
				<ref bean="kpsystem.hiv.report.eligibleForArt" />
				<ref bean="kpsystem.hiv.report.onSecondLineArt" />
				<ref bean="kpsystem.hiv.report.neverTakenCtxOrDapsone" />
				<ref bean="kpsystem.hiv.report.needsViralLoad" />
				<ref bean="kpsystem.etl.common.report.viralSuppression" />
			</set>
		</property>
		<property name="fragments">
			<map>
				<entry key="enrollment-summary" value="kpsystem:program/hiv/hivEnrollmentSummary" />
				<entry key="care-panel" value="kpsystem:program/hiv/hivCarePanel" />
				<entry key="completion-summary" value="kpsystem:program/hiv/hivCompletionSummary" />
			</map>
		</property>
		<property name="order" value="100" />
	</bean>

	<!-- ===================== Identifiers ===================== -->

